{
  "name": "react-redux-webpack-boilerplate",
  "description": "My current boilerplate",
  "private": true,
  "engines": {
    "node": "6.0.0"
  },
  "scripts": {
    "server:dev": "cross-env NODE_ENV=development node dev-server/index.js & NODE_ENV=development webpack-dev-server --config client/builder/webpack/dev.js --hot --inline",
    "server:dev:api": "cross-env NODE_ENV=development node dev-server/index.js",
    "server:dev:webpack": "cross-env NODE_ENV=development webpack-dev-server --config client/builder/webpack/dev.js --hot --inline",
    "server": "NODE_ENV=production node dev-server/index.js",
    "build": "NODE_ENV=production webpack -p --config client/builder/webpack/prod.js"
  },
  "dependencies": {
    "autoprefixer": "^6.5.3",
    "axios": "0.15.2",
    "babel-core": "6.11.4",
    "babel-loader": "6.2.4",
    "babel-plugin-syntax-trailing-function-commas": "^6.20.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.11.1",
    "cross-env": "^3.1.4",
    "css-loader": "0.24.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.24.1",
    "imports-loader": "^0.6.5",
    "jquery": "^3.1.1",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lodash": "^4.17.2",
    "path": "^0.12.7",
    "postcss-loader": "^1.1.1",
    "query-string": "^4.3.1",
    "react": "15.3.2",
    "react-dom": "15.3.1",
    "react-overlays": "^0.6.10",
    "react-redux": "5.0.1",
    "react-router-dom": "^4.0.0",
    "redux": "3.5.2",
    "style-loader": "0.13.1",
    "svg-inline-loader": "^0.7.1",
    "url-loader": "^0.5.7",
    "webpack": "1.13.1",
    "webpack-preset-babel-stage-2": "^0.2.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ],
    "plugins": [
      "syntax-trailing-function-commas",
      "transform-object-assign"
    ]
  }
}
