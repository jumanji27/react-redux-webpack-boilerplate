{
  "name": "react-redux-webpack-boilerplate",
  "description": "Our current boilerplate",
  "private": true,
  "engines": {
    "node": "8.4.0"
  },
  "scripts": {
    "server:dev": "cross-env NODE_ENV=development node dev-server/index.js & NODE_ENV=development webpack-dev-server --config client/builder/webpack/dev.js --hot --inline --host 0.0.0.0",
    "server:dev:api": "cross-env NODE_ENV=development node dev-server/index.js",
    "server:dev:webpack": "cross-env NODE_ENV=development webpack-dev-server --config client/builder/webpack/dev.js --hot --inline --host 0.0.0.0",
    "server": "cross-env NODE_ENV=production node dev-server/index.js",
    "build": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js -p --config client/builder/webpack/prod.js",
    "test": "cross-env jest",
    "lint": "cross-env node_modules/eslint/bin/eslint.js --ext .js .jsx ."
  },
  "dependencies": {
    "autoprefixer": "^7.1.2",
    "axios": "^0.16.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.29.0",
    "imports-loader": "^0.7.1",
    "jest": "^20.0.4",
    "jquery": "^3.2.1",
    "json-loader": "^0.5.7",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "lodash": "^4.17.4",
    "path": "^0.12.7",
    "postcss-loader": "^2.0.6",
    "query-string": "^5.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-overlays": "^0.7.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2",
    "style-loader": "^0.18.2",
    "svg-inline-loader": "^0.8.0",
    "url-loader": "^0.5.9",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.6.1",
    "webpack-preset-babel-stage-2": "^0.2.0",
    "eslint": "3.19.0",
    "eslint-plugin-react": "7.3.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-config-airbnb": "15.1.0",
    "file-loader": "0.5.1"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "react",
      "stage-2"
    ],
    "plugins": [
      "syntax-trailing-function-commas",
      "transform-object-assign"
    ]
  }
}
